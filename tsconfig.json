{
  "include": ["**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["@types/wicg-file-system-access", "vite/client"],
    "esModuleInterop": true,
    "moduleResolution": "Bundler",
    "module": "ES2022",
    "target": "ES2015",
    "downlevelIteration": true,
    "resolveJsonModule": true
  }
}
